  <SQLQuery>
    <![CDATA[SELECT INSEAM, size1, SUM (SIZE_QTY) size_qty
FROM (SELECT DISTINCT B.BPO_ID,
B.BPO,
BL.QTY SIZE_QTY,
SV.INSEAM,
SV.SIZE1,
mmsta.ratioplan_id
FROM MNJ_MFG_RATIOPLAN mmr,
MNJ_MFG_RATIOPLAN_STNS_ALL mmsta,
xx_om_BPO_info_T B,
XX_OM_SIZEWISE_L_T BL,
mnj_mfg_ratio_size_v sv
WHERE mmr.RATIOPLAN_ID = mmsta.RATIOPLAN_ID
AND mmsta.BPO_ID = B.BPO_ID
AND B.BPO_ID = BL.BPO_ID
AND BL.SIZE_NAME = sv.SIZE_INSEAM
--AND to_char(h.SOLINE_ID) = to_char(mmsta.STN_NO)
AND mmsta.ratioplan_id = :1
AND SV.INSEAM = :2
)
GROUP BY INSEAM, size1
ORDER BY LENGTH (size1), size1]]>
  </SQLQuery>
