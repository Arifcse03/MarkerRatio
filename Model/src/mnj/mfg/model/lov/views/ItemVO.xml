          Name="autoExecute"
          Value="false"/>
        <Property
          Name="allowConjunctionOverride"
          Value="true"/>
        <Property
          Name="showInList"
          Value="true"/>
        <Property
          Name="mode"
          Value="Basic"/>
      </CustomProperties>
    </Properties>
    <ViewCriteriaRow
      Name="vcrow16"
      UpperColumns="1">
      <ViewCriteriaItem
        Name="ItemVOCriteria_vcrow16_HeaderId"
        ViewAttribute="HeaderId"
        Operator="="
        Conjunction="AND"
        Value=":p_saleOrderID"
        GenerateIsNullClauseForBindVars="false"
        IsBindVarValue="true"
        Required="Optional"/>
      <ViewCriteriaItem
        Name="ItemCode"
        ViewAttribute="ItemCode"
        Operator="CONTAINS"
        Conjunction="AND"
        Value=":p_item"
        IsBindVarValue="true"
        Required="Optional"/>
      <ViewCriteriaItem
        Name="ItemVOCriteria_vcrow16_Description"
        ViewAttribute="Description"
        Operator="CONTAINS"
        Conjunction="AND"
        Value=":p_desc"
        IsBindVarValue="true"
