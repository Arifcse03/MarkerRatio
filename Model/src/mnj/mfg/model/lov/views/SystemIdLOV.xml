       L. color COLOUR,
        H.POC_ID
      
 FROM xx_om_poc_h_t H, xx_om_poc_L_t L, ar_customers ac
WHERE h.poc_id = L.poc_id
AND h.BUYER_ID = ac.CUSTOMER_ID
and  H.poc_id=4647 AND H.LC_UNIT =340
