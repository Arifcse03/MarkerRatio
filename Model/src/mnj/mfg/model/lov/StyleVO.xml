<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="StyleVO"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.mfg.model.lov.StyleVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="p_buyerId"
    Kind="where"
    Type="java.lang.String">
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="12"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <Variable
    Name="p_org_id"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression><![CDATA[adf.context.sessionScope.orgId]]></TransientExpression>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="12"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery>
    <![CDATA[SELECT DISTINCT mmoh.style style_name, 
                mmoh.season season,
                (SELECT DECODE(StyleLineEO.TRACK_BASIS,'Style Name',StyleLineEO.STYLE_NAME,'Style Number',StyleLineEO.STYLE_NUMBER)
       FROM PWC_MFG_BSS_STYLE_L StyleLineEO,
            pwc_mfg_bss_buyer_h pwcmfgbssbuyerh,
            PWC_MFG_BSS_SEASON_D SeasonDetailEO   
       WHERE StyleLineEO.STYLE_IDENTIFIER = mmoh.STYLE
       AND   pwcmfgbssbuyerh.BUYER_ID = mmoh.Buyer_d
       --AND   pwcmfgbssbuyerh.ORG_ID = mmoda.final_mfg_org_id
       AND   StyleLineEO.HEADER_ID = pwcmfgbssbuyerh.HEADER_ID
       AND   SeasonDetailEO.LINE_ID = StyleLineEO.LINE_ID 
       AND   rownum = 1)Current_Style,
       (SELECT SeasonDetailEO.SEASON_NAME
       FROM PWC_MFG_BSS_STYLE_L StyleLineEO,
            pwc_mfg_bss_buyer_h pwcmfgbssbuyerh,
            PWC_MFG_BSS_SEASON_D SeasonDetailEO   
       WHERE StyleLineEO.STYLE_IDENTIFIER = mmoh.STYLE
       AND   pwcmfgbssbuyerh.BUYER_ID = mmoh.Buyer_d
       --AND   pwcmfgbssbuyerh.ORG_ID = mmoda.final_mfg_org_id
       and   SeasonDetailEO.SEASON_NAME = mmoh.season
       AND   StyleLineEO.HEADER_ID = pwcmfgbssbuyerh.HEADER_ID
       AND   SeasonDetailEO.LINE_ID = StyleLineEO.LINE_ID 
       AND   rownum = 1)Current_Season
           FROM mnj_mfg_osp_dep_assign mmoda,
                mnj_mfg_osp_stn_line mmosl,
                mnj_mfg_osp_header mmoh,
                ar_customers ac
          WHERE mmoda.stn_line_id = mmosl.stn_line_id
            AND mmosl.header_id = mmoh.header_id
            AND ac.customer_id = mmoh.buyer_d
            AND mmoh.style IS NOT NULL
            AND mmoda.final_mfg_org_id = :p_org_id
            AND ac.customer_id = :p_buyerId
            AND MMODA.FINAL_DEPARTMENT = 'Cutting']]>
  </SQLQuery>
  <ViewAttribute
    Name="StyleName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="255"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="STYLE_NAME"
    Expression="STYLE_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="500"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="mnj.mfg.model.lov.StyleVO.StyleName_LABEL"/>
        <DISPLAYWIDTH
          Value="37"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="Season"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="500"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="SEASON"
    Expression="SEASON"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="500"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="37"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="CurrentStyle"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CURRENT_STYLE"
    Expression="CURRENT_STYLE"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="mnj.mfg.model.lov.StyleVO.CurrentStyle_LABEL"/>
        <DISPLAYWIDTH
          Value="12"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="CurrentSeason"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="250"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CURRENT_SEASON"
    Expression="CURRENT_SEASON"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="250"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <LABEL
          ResId="mnj.mfg.model.lov.StyleVO.CurrentSeason_LABEL"/>
        <DISPLAYWIDTH
          Value="12"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnj.mfg.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
