          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </Variable>
  <SQLQuery>
    <![CDATA[select distinct ooha.HEADER_ID,
       ooha.ORDER_NUMBER,
       ooha.org_id,   
       msib.SEGMENT1 || '-' || msib.SEGMENT2 || '-' || msib.SEGMENT3 || '-' ||
       msib.SEGMENT4 || '-' || msib.SEGMENT5||'-'|| msib.SEGMENT6 item_code,
       msib.DESCRIPTION,
       msib.INVENTORY_ITEM_ID       
  from oe_order_headers_all         ooha,
       oe_order_lines_all           oola,
       mtl_system_items_b           msib,
       org_organization_definitions ood
 where ooha.HEADER_ID = oola.HEADER_ID
   and msib.INVENTORY_ITEM_ID = oola.INVENTORY_ITEM_ID
   and ood.organization_id = msib.ORGANIZATION_ID
   and ooha.org_id = ood.operating_unit]]>
  </SQLQuery>
